    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "json-schema-to-typescript": "^13.1.2",
    "nodemon": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
