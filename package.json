{
  "name": "komorebi-ui",
  "productName": "komorebi-ui",
  "version": "1.0.0-beta.7",
  "description": "My Electron application description",
  "main": "dist/background-bundle/index.js",
  "scripts": {
    "dev": "nodemon --config nodemon.json",
    "start": "electron-forge start",
    "package": "npm run build-komorebi && electron-forge package",
    "make": "npm run build-komorebi && electron-forge make",
    "publish": "npm run build-komorebi && electron-forge publish",
    "lint": "eslint ./**/** --fix --max-warnings 0",
    "build-komorebi": "cd ./komorebi && cargo build --locked --release --target x86_64-pc-windows-msvc",
    "postversion": "node scripts/update_changelog.mjs"
  },
  "keywords": [],
