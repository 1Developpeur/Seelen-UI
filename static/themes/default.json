{
  "$schema": "https://raw.githubusercontent.com/eythaann/Komorebi-UI/master/documentation/theme.schema.json",
  "info": {
    "displayName": "Default",
    "author": "eythaann",
    "description": "Default Seelen Theme"
  },
  "seelenweg": {
    "background": [
      {
        "opacity": 0.3,
        "filter": "saturate(0)",
        "backgroundImage": "url(\"data:image/svg+xml,%3Csvg viewBox='0 0 250 250' xmlns='http://www.w3.org/2000/svg'%3E%3Cfilter id='noiseFilter'%3E%3CfeTurbulence type='fractalNoise' baseFrequency='10' numOctaves='3' stitchTiles='stitch '/%3E%3C/filter%3E%3Crect width='100%25' height='100%25' filter='url(%23noiseFilter)'/%3E%3C/svg%3E\")",
        "backgroundSize": "cover",
        "borderRadius": "10px"
      },
      {
        "opacity": 0.6,
        "backgroundColor": "var(--color-gray-100)",
        "borderRadius": "10px"
      }
    ],
    "items": {
      "background": [
        {
          "backgroundColor": "var(--color-gray-100)",
          "borderRadius": "25%",
          "boxShadow": "0px 2px 3px 0px rgba(0, 0, 0, 0.5)"
        }
      ]
    },
    "contextMenu": {
      "background": [
        {
          "backgroundColor": "var(--color-gray-200)",
          "borderRadius": "10px"
        }
      ],
      "content": {
        "padding": "6px"
      }
    },
    "preview": {
      "background": [
        {
          "backgroundColor": "var(--color-gray-200)",
          "borderRadius": "10px"
        }
      ],
      "content": {
        "padding": "10px",
        "borderRadius": "10px"
      },
      "items": {
        "content": {
          "padding": "6px 10px 10px 10px",
          "borderRadius": "10px"
        },
        "title": {
          "fontSize": "14px",
          "fontWeight": "600",
          "color": "var(--color-gray-900)",
          "margin": "0 0 8px 0"
        },
        "image": {
          "borderRadius": "10px",
          "border": "1px solid var(--color-gray-300)"
        }
      }
    }
  }
}