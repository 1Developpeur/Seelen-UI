{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "target": "ES2020",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": ["react", "readable-types", "node", "jest"],
    "skipLibCheck": true,
    "esModuleInterop": true,
  }
}